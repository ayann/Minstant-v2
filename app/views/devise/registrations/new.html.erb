<div class="row" id="devise_sign_up_form">
	<%= devise_error_messages! %>
    <h1>Inscription</h1>
	<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
		<div class="form-inputs">
			<%= f.input :companyName, label: 'Nom de l\'entreprise',:autofocus => true %>
		</div>
		<div class="form-inputs">
			<%= f.input :address, label: 'Adresse de l\'entreprise' %>
		</div>
		<div class="form-inputs">
			<%= f.input :tel, label: 'Numéro de télephone de l\'entreprise'%>
		</div>
		<div class="form-inputs">
			<%= f.input :email %>
		</div>
		<div class="form-inputs">
			<%= f.input :password, label: 'Mot de passe' %>
		</div>
		<div class="form-inputs">
			<%= f.input :password_confirmation, label: 'Repeter mot de passe' %>
		</div>
	    <div class="form-actions">
	    	<%= f.button :submit, "S'incrire",:class => 'button tiny expand' %>
	    </div>
	<% end %>
<div>
       